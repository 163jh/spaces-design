{
    "APPLICATION": {
        "$enable-rule": "always",
        "ABOUT": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 1
            },
            "$enable-rule": "always-except-modal"
        },
        "HIDE_APPLICATION": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 7001
            },
            "$enable-rule": "always"
        },
        "HIDE_OTHER_APPLICATIONS": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 7002
            },
            "$enable-rule": "always"
        },
        "QUIT_APPLICATION": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 36
            },
            "$enable-rule": "always"
        }
    },
    "FILE": {
        "$enable-rule": "always",
        "NEW": {
            "$action": "documents.createNew",
            "$enable-rule": "always-except-modal"
        },
        "NEW_EXTENDED": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 10
            },
            "$enable-rule": "always-except-modal"
        },
        "NEW_FROM_TEMPLATE": {
            "$enable-rule": "always-except-modal"
        },
        "OPEN": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 20
            },
            "$enable-rule": "always-except-modal"
        },
        "CLOSE": {
            "$action": "documents.close",
            "$enable-rule": "have-document"
        },
        "SAVE": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 30
            },
            "$enable-rule": "dirty-document"
        },
        "SAVE_AS": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 32
            },
            "$enable-rule": "have-document"
        },
        "REVERT": {
            "$action": "documents.revertCurrentDocument",
            "$payload": {
                "commandID": 34
            },
            "$enable-rule": "dirty-document"
        },
        "OPEN_RECENT": {
            "$enable-rule": "always-except-modal"
        },
        "PLACE_LINKED": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 3090
            },
            "$enable-rule": "have-document"
        },
        "PLACE_EMBEDDED": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 1032
            },
            "$enable-rule": "have-document"
        },
        "PACKAGE": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 3092
            },
            "$enable-rule": "have-document,have-linked"
        },
        "PRINT": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 2101
            },
            "$enable-rule": "have-document"
        }
    },
    "EDIT": {
        "$enable-rule": "always",
        "CUT": {
            "$action": "edit.cut",
            "$payload": {
                "preserveFocus": true
            },
            "$enable-rule": "always-except-modal"
        },
        "COPY": {
            "$action": "edit.copy",
            "$payload": {
                "preserveFocus": true
            },
            "$enable-rule": "always-except-modal"
        },
        "PASTE": {
            "$action": "edit.paste",
            "$payload": {
                "preserveFocus": true
            },
            "$enable-rule": "always-except-modal"
        },
        "UNDO": {
            "$action": "edit.undo",
            "$enable-rule": "have-document,earlier-history"
        },
        "REDO": {
            "$action": "edit.redo",
            "$enable-rule": "have-document,later-history"
        },
        "SELECT_ALL": {
            "$action": "edit.selectAll",
            "$payload": {
                "preserveFocus": true
            },
            "$enable-rule": "have-document"
        },
        "DESELECT": {
            "$action": "layers.deselectAll",
            "$enable-rule": "layer-selected"
        },
        "DELETE": {
            "$action": "layers.deleteSelected",
            "$enable-rule": "layer-selected"
        }
    },
    "LAYER": {
        "$enable-rule": "always",
        "TRANSFORM": {
            "$enable-rule": "layer-selected",
            "TRANSFORM_ROTATE_180": {
                "$action": "transform.rotateLayersInCurrentDocument",
                "$payload": {
                    "angle": 180
                },
                "$enable-rule": "layer-selected"
            },
            "TRANSFORM_ROTATE_LEFT": {
                "$action": "transform.rotateLayersInCurrentDocument",
                "$payload": {
                    "angle": -90
                },
                "$enable-rule": "layer-selected"
            },
            "TRANSFORM_ROTATE_RIGHT": {
                "$action": "transform.rotateLayersInCurrentDocument",
                "$payload": {
                    "angle": 90
                },
                "$enable-rule": "layer-selected"
            }
        },
        "COMBINE": {
            "$enable-rule": "layer-selected-all-shapes",
            "COMBINE_UNION": {
                "$action": "shapes.combineUnionSelectedInCurrentDocument",
                "$enable-rule": "layers-selected-all-shapes"
            },
            "COMBINE_SUBTRACT": {
                "$action": "shapes.combineSubtractSelectedInCurrentDocument",
                "$enable-rule": "layers-selected-all-shapes"
            },
            "COMBINE_INTERSECT": {
                "$action": "shapes.combineIntersectSelectedInCurrentDocument",
                "$enable-rule": "layers-selected-all-shapes"
            },
            "COMBINE_DIFFERENCE": {
                "$action": "shapes.combineDifferenceSelectedInCurrentDocument",
                "$enable-rule": "layers-selected-all-shapes"
            }
        },
        "CREATE_ARTBOARD" : {
            "$action": "layers.createArtboard",
            "$enable-rule": "have-document"
        }
    },
    "ARRANGE": {
        "$enable-rule": "always",
        "LOCK_LAYER": {
            "$action": "layers.lockSelectedInCurrentDocument",
            "$enable-rule": "layer-selected"
        },
        "UNLOCK_LAYER": {
            "$action": "layers.unlockSelectedInCurrentDocument",
            "$enable-rule": "layer-selected"
        },
        "FLIP_HORIZONTAL": {
            "$action": "transform.flipXCurrentDocument",
            "$enable-rule": "layer-selected"
        },
        "FLIP_VERTICAL": {
            "$action": "transform.flipYCurrentDocument",
            "$enable-rule": "layer-selected"
        },
        "SWAP_POSITION": {
            "$action": "transform.swapLayersCurrentDocument",
            "$enable-rule": "layers-selected-2,no-background,no-nesting"
        },
        "GROUP_LAYERS": {
            "$action": "layers.groupSelectedInCurrentDocument",
            "$enable-rule": "layer-selected"
        },
        "DISTRIBUTE": {
            "$enable-rule": "layers-selected-3+,no-background,no-nesting",
            "DISTRIBUTE_HORIZONTAL": {
                "$action": "transform.distributeX",
                "$enable-rule": "layers-selected-3+,no-background,no-nesting"
            },
            "DISTRIBUTE_VERTICAL": {
                "$action": "transform.distributeY",
                "$enable-rule": "layers-selected-3+,no-background,no-nesting"
            }
        },
        "ALIGN": {
            "$enable-rule": "layers-selected-2+,no-background,no-nesting",
            "ALIGN_LEFT": {
                "$action": "transform.alignLeft",
                "$enable-rule": "layers-selected-2+,no-background,no-nesting"
            },
            "ALIGN_CENTER": {
                "$action": "transform.alignHCenter",
                "$enable-rule": "layers-selected-2+,no-background,no-nesting"
            },
            "ALIGN_RIGHT": {
                "$action": "transform.alignRight",
                "$enable-rule": "layers-selected-2+,no-background,no-nesting"
            },
            "ALIGN_TOP": {
                "$action": "transform.alignTop",
                "$enable-rule": "layers-selected-2+,no-background,no-nesting"
            },
            "ALIGN_MIDDLE": {
                "$action": "transform.alignVCenter",
                "$enable-rule": "layers-selected-2+,no-background,no-nesting"
            },
            "ALIGN_BOTTOM": {
                "$action": "transform.alignBottom",
                "$enable-rule": "layers-selected-2+,no-background,no-nesting"
            }
        }
    },
    "VIEW": {
        "$enable-rule": "always",
        "ZOOM_IN": {
            "$action": "ui.zoomInOut",
            "$payload": {
                "zoomIn": true,
                "preserveFocus": true
            },
            "$enable-rule": "have-document"
        },
        "ZOOM_OUT": {
            "$action": "ui.zoomInOut",
            "$payload": {
                "zoomIn": false,
                "preserveFocus": true
            },
            "$enable-rule": "have-document"
        },
        "FIT_TO_WINDOW": {
            "$action": "ui.centerOn",
            "$payload": {
                "on": "document",
                "zoomInto": true,
                "preserveFocus": true
            },
            "$enable-rule": "have-document"
        },
        "ACTUAL_SIZE": {
            "$action": "ui.zoom",
            "$payload": {
                "zoom": 1,
                "pan": true,
                "preserveFocus": true
            },
            "$enable-rule": "have-document"
        },
        "ZOOM_TO_SELECTION": {
            "$action": "ui.centerOn",
            "$payload": {
                "on": "selection",
                "zoomInto": true,
                "preserveFocus": true
            },
            "$enable-rule": "layer-selected"
        },
        "CENTER_SELECTION": {
            "$action": "ui.centerOn",
            "$payload": {
                "on": "selection",
                "zoomInto": false,
                "preserveFocus": true
            },
            "$enable-rule": "layer-selected"
        },
        "SWITCH_TO_CLASSIC": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 5999
            },
            "$enable-rule": "always"
        },
        "TOGGLE_GUIDES": {
            "$action": "documents.toggleGuidesVisibility",
            "$enable-rule": "have-document"
        },
        "TOGGLE_SMART_GUIDES": {
            "$action": "documents.toggleSmartGuidesVisibility",
            "$enable-rule": "have-document"
        }
    },
    "WINDOW": {
        "$enable-rule": "always",
        "NEXT_DOCUMENT": {
            "$action": "documents.selectNextDocument",
            "$enable-rule": "multiple-documents"
        },
        "PREVIOUS_DOCUMENT": {
            "$action": "documents.selectPreviousDocument",
            "$enable-rule": "multiple-documents"
        }
    },
    "HELP": {
        "$enable-rule": "always",
        "RUN_TESTS": {
            "$action": "menu.runTests",
            "$enable-rule": "always-except-modal"
        },
        "ACTION_FAILURE": {
            "$action": "menu.actionFailure",
            "$enable-rule": "always-except-modal"
        },
        "RESET_FAILURE": {
            "$action": "menu.resetFailure",
            "$enable-rule": "always-except-modal"
        },
        "UPDATE_CURRENT_DOCUMENT": {
            "$action": "documents.updateCurrentDocument",
            "$enable-rule": "have-document"
        },
        "RESET_RECESS": {
            "$action": "menu.resetRecess",
            "$enable-rule": "always-except-modal"
        },
        "OPEN_FIRST_LAUNCH": {
            "$action": "help.openFirstLaunch",
            "$enable-rule": "always-except-modal"
        }
    }
}
