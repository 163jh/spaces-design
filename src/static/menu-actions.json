{
    "APPLICATION": {
        "ABOUT": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 1
            }
        },
        "QUIT_APPLICATION": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 36
            }
        }
    },
    "FILE": {
        "NEW": {
            "$action": "documents.createNew"
        },
        "NEW_EXTENDED": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 10
            }
        },
        "NEW_FROM_TEMPLATE": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 10
            }
        },
        "OPEN": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 20
            }
        },
        "CLOSE": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 31
            }
        },
        "SAVE": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 30
            }
        },
        "SAVE_AS": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 32
            }
        }
    },
    "EDIT": {
        "CUT": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 103
            }
        },
        "COPY": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 104
            }
        },
        "PASTE": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 105
            }
        },
        "UNDO": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 1961
            }
        },
        "REDO": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 1962
            }
        },
        "DESELECT": {
            "$action": "layers.deselectAll"
        },
        "DELETE": {
            "$action": "layers.deleteSelected"
        }
    },
    "LAYER": {
        "TRANSFORM": {
            "TRANSFORM_ROTATE_180": {
                "$action": "transform.rotateLayersInCurrentDocument",
                "$payload": {
                    "angle": 180
                }
            },
            "TRANSFORM_ROTATE_LEFT": {
                "$action": "transform.rotateLayersInCurrentDocument",
                "$payload": {
                    "angle": -90
                }
            },
            "TRANSFORM_ROTATE_RIGHT": {
                "$action": "transform.rotateLayersInCurrentDocument",
                "$payload": {
                    "angle": 90
                }
            }
        }
    },
    "ARRANGE": {
        "LOCK_LAYER": {
            "$action": "layers.lockSelectedInCurrentDocument"
        },
        "UNLOCK_LAYER": {
            "$action": "layers.unlockSelectedInCurrentDocument"
        },
        "FLIP_HORIZONTAL": {
            "$action": "transform.flipXCurrentDocument"
        },
        "FLIP_VERTICAL": {
            "$action": "transform.flipYCurrentDocument"
        },
        "SWAP_POSITION": {
            "$action": "transform.swapLayersCurrentDocument"
        },
        "GROUP_LAYERS": {
            "$action": "layers.groupSelectedInCurrentDocument"
        }
    },
    "VIEW": {
        "ZOOM_IN": {
            "$action": "ui.zoomInOut",
            "$payload": {
                "zoomIn": true
            }
        },
        "ZOOM_OUT": {
            "$action": "ui.zoomInOut",
            "$payload": {
                "zoomIn": false
            }
        },
        "FIT_TO_WINDOW": {
            "$action": "ui.centerOn",
            "$payload": {
                "on": "document",
                "zoomInto": true
            }
        },
        "ACTUAL_SIZE": {
            "$action": "ui.zoom",
            "$payload": {
                "zoom": 1,
                "pan": true
            }
        },
        "ZOOM_TO_SELECTION": {
            "$action": "ui.centerOn",
            "$payload": {
                "on": "selection",
                "zoomInto": true
            }
        },
        "CENTER_SELECTION": {
            "$action": "ui.centerOn",
            "$payload": {
                "on": "selection",
                "zoomInto": false
            }
        },
        "SWITCH_TO_CLASSIC": {
            "$action": "menu.native",
            "$payload": {
                "commandID": 5999
            }
        }
    },
    "WINDOW": {
        "NEXT_DOCUMENT": {
            "$action": "documents.selectNextDocument"
        },
        "PREVIOUS_DOCUMENT": {
            "$action": "documents.selectPreviousDocument"
        }
    },
    "HELP": {
        "RUN_TESTS": {
            "$action": "menu.runTests"
        }
    }
}
